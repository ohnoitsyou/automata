  <h1>{{title}}</h1>
  <p>Welcome to {{title}}</p>
  <p>The plugins are located at {{pluginDir}}</p>
  <script src="https://ajax.googleapis.com/ajax/libs/prototype/1.7.2.0/prototype.js" type="text/javascript"></script>
  <script>
    var loadedPlugins;
    function renderDisplays() {
      loadedPlugins.forEach(function(plugin) {
        new Ajax.Request("/api/" + plugin + "/render", {
            method: "get",
            onSuccess: function(data) {
              //console.log(data.responseText);
              var link = new Element('link', {src: "{{pluginDir}}/" + plugin + data.responseText, 'rel': 'import'});
              $$('head')[0].insert(link);
            }
        });
      });
    }
    new Ajax.Request("/loader/pluginList", {
      method: 'get',
      onSuccess: function(data) {
        loadedPlugins = JSON.parse(data.responseJSON);
        renderDisplays();
      }
    });
    document.observe('dom:loaded', function() {
    });
  </script>
  <div id="elements"></div>
